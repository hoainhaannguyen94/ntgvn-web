{
  "name": "ntgvn-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build_analyze": "ng build --stats-json",
    "analyze": "npm run build_stats-json & webpack-bundle-analyzer dist/fsd-fe/stats.json",
    "dev": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng serve",
    "build_dev": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --base-href / --configuration development",
    "build_prod": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --base-href / --configuration production",
    "docker_network": "docker network create ntgvn-network",
    "docker_ubuntu": "docker run --network ntgvn-network --name ntgvn-ubuntu -p 80:8080 -p 443:8443 -p 22:22 -itd ubuntu:latest",
    "docker_ntgvn-web-build": "docker build . -t ntgvn-web:latest",
    "docker_ntgvn-web-run": "docker run -d -p 80:80 --network ntgvn-network --name ntgvn-web ntgvn-web:latest",
    "docker_compose": "docker-compose up"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.1.4",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.1.4",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@angular/service-worker": "^16.1.0",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/list": "^6.1.8",
    "@fullcalendar/multimonth": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@ngneat/transloco": "^4.3.0",
    "@ngrx/store": "^16.1.0",
    "animate.css": "^4.1.1",
    "highcharts": "^11.1.0",
    "highcharts-angular": "^3.1.2",
    "immutable": "^4.3.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "ngx-toastr": "^17.0.2",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.7.1",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zod": "^3.21.4",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular/cli": "~16.1.4",
    "@angular/compiler-cli": "^16.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/leaflet": "^1.9.3",
    "@types/lodash": "^4.14.195",
    "@types/luxon": "^3.3.0",
    "@types/uuid": "^9.0.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}